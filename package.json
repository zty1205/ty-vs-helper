{
  "name": "ty-helper",
  "displayName": "Ty Helper",
  "description": "Ty Helper",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:ty-helper.DeleteLocalBranch",
    "onLanguage:json"
  ],
  "main": "./src/index.js",
  "contributes": {
    "commands": [
      {
        "command": "ty-helper.DeleteLocalBranch",
        "title": "删除本地分支"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorFocus",
          "command": "ty-helper.DeleteLocalBranch",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "build": "vsce package",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.46.0",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}
