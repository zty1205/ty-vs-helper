{
  "name": "ty-helper",
  "displayName": "Ty Helper",
  "description": "Ty Helper",
  "version": "0.0.3",
  "icon": "images/icon.png",
  "author": "zty<https://github.com/zty1205>",
  "repository": {
    "type": "git",
    "url": "https://github.com/zty1205/ty-vs-helper"
  },
  "homepage": "https://github.com/zty1205/ty-vs-helper",
  "galleryBanner": {
    "color": "#C80000",
    "theme": "dark"
  },
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "license": "MIT",
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Other",
    "Extension Packs"
  ],
  "keywords": [
    "vscode",
    "helper"
  ],
  "activationEvents": [
    "onCommand:ty-helper.DeleteLocalBranch",
    "onCommand:ty-helper.GitPush",
    "onLanguage:json"
  ],
  "main": "./src/index.js",
  "contributes": {
    "commands": [
      {
        "command": "ty-helper.DeleteLocalBranch",
        "title": "删除本地分支"
      },
      {
        "command": "ty-helper.GitPush",
        "title": "Git 一键 Push"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorFocus",
          "command": "ty-helper.DeleteLocalBranch",
          "group": "navigation"
        },
        {
          "when": "editorFocus",
          "command": "ty-helper.GitPush",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "title": "Ty Helper",
      "properties": {
        "ty-helper.GitPush.commitizen": {
          "type": "boolean",
          "default": false,
          "description": "是否需要commitizen"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "build": "vsce package",
    "publish": "vsce publish",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.46.0",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "publisher": "tengyuezhang"
}
